{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "eol-last": "off",
    "indent": [
      "error",
      2,
      {
        "MemberExpression": 1
      }
    ],
    "no-tabs": "off",
    "semi": ["error", "always"],
    "comma-dangle": ["error", "never"],
    "comma-style": ["error", "last"],
    "consistent-return": "off",
    "quotes": ["error", "double"],
    "no-console": "off"
  }
}
